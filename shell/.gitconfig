[user]
	name = Harry Jeffery
	email = harry@exec64.co.uk
[core]
	editor = vim
	autocrlf = input
[color]
	ui = true
[push]
	default = simple
[alias]
  co = checkout
  ci = commit
  s = status -s -b
  st = status
  sh = show
  sa = stash
  b = branch
  bb = branch --verbose --verbose
  bu = !git fetch --all --prune && git branch --verbose --verbose
  l = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate -n 15
  ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
  ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
  lg = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --graph
  d = diff
  ds = diff --staged
  a = add
  pl = !git fetch --all --prune && git rebase FETCH_HEAD
  ps = push
  f = fetch --all --prune
  rb = rebase
  rbi = rebase --interactive --autosquash
  rbh = rebase --interactive --autosquash HEAD~10
  fix = commit --fixup
